	<!--ControlledMovieClip-->
	<controlledMovieClip>
		<item url="assets/{resVersion}/ani/animations.swf">
			<animations.Female /> <animations.Light /> <animations.monsters.Skeleton /> <animations.monsters.Zombie />
		</item>
		<item url="assets/{resVersion}/swf/aaa.swf">
			<aaa.bbb />
		</item>
		<item url="assets/{resVersion}/swf/bbb.swf">
			<aaa.ccc />
		</item>
		
	</controlledMovieClip>
