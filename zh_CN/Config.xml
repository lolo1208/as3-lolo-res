<?xml version="1.0" encoding="utf-8"?>
<config>
	<!--资源库语言、版本-->
	<resVersion value="zh_CN" />
	
	<!--socket后端服务器地址-->
	<socketServiceUrl value="10.6.90.13:1122" />
	<!--http后端服务器地址-->
	<httpServiceUrl value="http://127.0.0.1/service/" />
	
	<!--后端服务器地址列表，登录界面用（测试阶段）-->
